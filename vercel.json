{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "buildCommand": "./build.sh",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install --force",
  "public": true,
  "github": {
    "silent": false
  }
}
